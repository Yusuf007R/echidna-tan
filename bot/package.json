{
	"name": "bot",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"prebuild": "rimraf dist",
		"predev": "pnpm run check",
		"dev": "node esbuild.config.js watch",
		"start": "pnpm run build && node ./dist/index.js ",
		"build": "node esbuild.config.js",
		"build:prod": "NODE_ENV=production node esbuild.config.js",
		"lint": "biome check --write src/*",
		"format": "biome format --write src/*",
		"type-check": "tsc --noEmit",
		"fix": "pnpm run format && pnpm run lint",
		"check": "pnpm run type-check && pnpm run fix",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:clear": "tsx db-actions.ts clear",
		"prepare": "cd .. && husky bot/.husky"
	},
	"author": "Yusuf Rawat",
	"license": "ISC",
	"dependencies": {
		"@discord-player/extractor": "^7.1.0",
